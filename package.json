{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:optimized": "NODE_OPTIONS='--max-old-space-size=4096 --max-semi-space-size=512' next dev --turbopack",
    "dev:production": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' next dev --turbopack",
    "build": "next build",
    "build:optimized": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "start": "next start",
    "start:optimized": "NODE_OPTIONS='--max-old-space-size=4096' next start",
    "lint": "next lint",
    "export": "next build",
    "deploy": "npm run export && touch out/.nojekyll",
    "health": "node -e \"console.log('üè• System Health:'); console.log('Node version:', process.version); console.log('Memory:', Math.round(process.memoryUsage().heapUsed/1024/1024) + 'MB'); console.log('Platform:', process.platform);\"",
    "optimize": "./optimize-dashboard.sh",
    "dashboard:nvfuser": "npm run dev:optimized & sleep 4 && open 'http://localhost:3000/dashboard?repo=Fuser'",
    "dashboard:thunder": "npm run dev:optimized & sleep 4 && open 'http://localhost:3000/dashboard?repo=lightning-thunder'",
    "dashboard:te": "npm run dev:optimized & sleep 4 && open 'http://localhost:3000/dashboard?repo=TransformerEngine'"
  },
